    "@parcel/types": "^2.0.0-rc.0",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "terser": "^5.7.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/minimatch": "^3.0.5",
    "minimatch": "^3.0.4"
  }
}
